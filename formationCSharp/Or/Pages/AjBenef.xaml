<PageFunction x:Class="Or.Pages.AjBenef"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:sys="clr-namespace:System;assembly=mscorlib"
      x:TypeArguments="sys:Int64"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Or.Pages"
      xmlns:ec="clr-namespace:Or.Business"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AjBenef" KeepAlive="True" Cursor="Hand">

    <Grid>
        <TextBlock x:Name="Titre" HorizontalAlignment="Left" Margin="100,85,0,0" TextWrapping="Wrap" Text="Ajouter un bénéficiaire" VerticalAlignment="Top" FontSize="20" FontWeight="SemiBold"/>

        <Label x:Name="labelE" Content="N de Compte de Nouveau bénificiaire" HorizontalAlignment="Left" Margin="37,181,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.237,0.385"/>
        <TextBox x:Name="NumCompte"
                   HorizontalAlignment="Left"
                   Margin="37,225,0,0"
                   TextWrapping="Wrap"
                   TextAlignment="Center"
                   VerticalAlignment="Top"
                   Height="22" Width="189" />


        <Button x:Name="AjouterBenif" Content="Ajouter Un Bénéficiaire" HorizontalAlignment="Left" Margin="520,263,0,0" VerticalAlignment="Top" Height="35" Width="142" Click="AjouterUnBenif"/>
        <Button x:Name="Retour" Content="Retour" HorizontalAlignment="Left" Margin="100,330,0,0" VerticalAlignment="Top" Height="30" Width="80" Click="Retour_Click"/>

    </Grid>
</PageFunction>
